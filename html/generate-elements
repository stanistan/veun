#!/bin/bash


cat << EOF > elements.go
//
// Code generated DO NOT EDIT.
//
// Generated in $GOPACKAGE/$GOFILE:$GOLINE using $1
package $GOPACKAGE

import "github.com/stanistan/veun"

EOF

for el in $(cat $1)
do
  name="$(echo $el | tr '[:upper:]' '[:lower:]')"
  cat << EOF >> elements.go

// $el constructs a veun representation of a $name.
//
// This function is autogenerated.
func $el(attrs Attrs, content ...veun.AsView) veun.AsView {
  return El("$name", attrs, veun.Views(content))
}
EOF
done
